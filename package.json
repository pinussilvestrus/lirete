{
  "name": "lirete",
  "version": "1.0.0",
  "description": "Web Plotter with linreg.py",
  "homepage": "https://github.com/pinussilvestrus/lirete#readme",
  "bugs": {
    "url": "https://github.com/pinussilvestrus/lirete/issues"
  },
  "license": "ISC",
  "author": "pinussilvestrus",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinussilvestrus/lirete.git"
  },
  "scripts": {
    "build": "webpack -d --watch",
    "cp:index": "cp src/static/index.html dist/",
    "dev": "npm run cp:index && NODE_ENV=local npm run build ",
    "setup:python": "python -m pip install numpy",
    "production": "NODE_ENV=production npm run cp:index && webpack -d",
    "start": "node app.js",
    "startd": "nodemon app.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register,js:./test/css-modules-compiler.js test/ --recursive",
    "windev": "npm run cp:index && set NODE_ENV=local&& npm run build",
    "rm:temp": "rm dist/temp/*.txt && rm dist/temp/*.png"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.25.0",
    "css-modules-require-hook": "^4.0.5",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "express-urlrewrite": "^1.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "feathers": "^2.0.3",
    "feathers-authentication": "^0.7.11",
    "feathers-authentication-client": "^0.1.1",
    "feathers-client": "^1.7.2",
    "feathers-configuration": "^0.4.1",
    "feathers-hooks": "^1.6.1",
    "feathers-python": "^0.1.0",
    "feathers-sequelize": "^1.4.0",
    "feathers-subscriptions-manager": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "node-sass": "^3.13.1",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.0.0",
    "randomstring": "^1.1.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-komposer": "^1.13.1",
    "react-loader": "^2.4.0",
    "react-player": "^0.12.0",
    "react-router": "^3.0.0",
    "react-s-alert": "^1.2.1",
    "request": "^2.76.0",
    "request-promise": "^4.1.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "sequelize": "^3.28.0",
    "serve-favicon": "^2.3.2",
    "socket.io-client": "^1.5.1",
    "sqlite3": "^3.1.8",
    "style-loader": "^0.13.1",
